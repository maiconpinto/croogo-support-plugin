{
    "name": "maiconpinto/croogo-support-plugin",
    "description": "Support plugin for Croogo",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Support\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Support\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
